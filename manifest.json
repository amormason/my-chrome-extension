{
    "manifest_version": 3,
    "name": "Auto Refresh Specific Tab",
    "version": "1.0",
    "permissions": [
        "tabs",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}